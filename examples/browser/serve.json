{
  "public": ".",
  "cleanUrls": false,
  "trailingSlash": false,
  "directoryListing": true,
  "symlinks": true,
  "unlisted": [
    "node_modules",
    ".gitignore",
    "config.json",
    "package.json",
    "package-lock.json",
    "pnpm-lock.yaml",
    "yarn.lock",
    "README.md",
    "serve.json"
  ],
  "headers": [
    {
      "source": "**/*.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    },
    {
      "source": "**/*.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}
