{
  "name": "jaaq-sdk-js",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsup",
    "dev": "dotenvx run -f .env.development -- tsup --watch",
    "build:dev": "dotenvx run -f .env.development -- tsup",
    "build:staging": "dotenvx run -f .env.staging -- tsup",
    "build:prod": "dotenvx run -f .env.production -- tsup",
    "test": "vitest run --exclude=tests/build",
    "test:watch": "vitest --exclude=tests/build",
    "test:smoke:dev": "dotenvx run -f .env.development -- vitest run tests/build",
    "test:smoke:staging": "dotenvx run -f .env.staging -- vitest run tests/build",
    "test:smoke:prod": "dotenvx run -f .env.production -- vitest run tests/build",
    "test:smoke:ci": "vitest run tests/build",
    "check-paths-in-bundle": "grep -R -nE --exclude='*.map' '@core/|@resources/' dist || echo 'OK no aliases found'",
    "openapi:gen": "openapi-typescript ./openapi.json -o ./src/gen/openapi.ts",
    "audit:moderate": "pnpm audit --audit-level=moderate"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.17.1",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.51.0",
    "@esbuild-plugins/tsconfig-paths": "^0.1.2",
    "@types/node": "^24.5.2",
    "msw": "^2.11.3",
    "openapi-typescript": "^7.9.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
